{
  "name": "@tomassabol/cdk-template",
  "version": "2.18.3-beta.1",
  "description": "tomassabol CDK Template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "bin": {
    "cdk-template-init": "bin/cdk-template-init.js"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "examples/**/*"
  ],
  "scripts": {
