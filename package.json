{
  "name": "@tomassabol/cdk-template",
  "version": "2.18.4",
  "description": "tomassabol CDK Template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "bin": {
    "cdk-template-init": "bin/cdk-template-init.js"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "examples/**/*"
  ],
  "scripts": {
    "test": "jest",
    "prepare": "husky install && rm -rf dist && tsc -b tsconfig-build.json",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hi@tomassabol.com",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomassabol/cdk-template.git"
  },
  "docs": "https://github.com/tomassabol/cdk-template#readme",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.5",
    "@types/aws-lambda": "^8.10.159",
    "@types/aws-sdk": "^2.7.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "esbuild": "^0.25.12",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-markdown": "^3.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^2.8.8",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.232.0",
    "constructs": "^10.4.3",
    "source-map-support": "^0.5.21"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7"
  },
  "overrides": {
    "braces": "^3.0.3",
    "micromatch": "^4.0.8"
