  },
  "docs": "https://github.com/tomassabol/cdk-template#readme",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/aws-lambda": "^8.10.106",
    "@types/aws-sdk": "^2.7.4",
    "@types/jest": "^29.1.1",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "esbuild": "^0.25.0",
    "esbuild-jest": "^0.4.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^16.2.6",
    "prettier": "^2.7.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.181.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
